{
  "manifest_version": 2,

  "name": "Mingle Card Opener",
  "description": "This extension makes it easy to open up a card in your Mingle project.",
  "version": "1.3",
  "permissions": [
    "tabs",
    "storage"
  ],
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
          
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}